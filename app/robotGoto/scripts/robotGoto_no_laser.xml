<application>
<name>robotGoto_no_laser</name>

<dependencies>
</dependencies>

<module>
   <name>robotGoto</name>
   <parameters>--no_stop_on_obstacles --from C:\software\navigation\app\robotGoto\conf\robotGoto_ikart.ini</parameters>
   <node>console</node>
</module>

<connection>
  <from>/robotGoto/control:o</from>
  <to>/baseControl/control:i</to>
  <protocol>udp</protocol>
</connection>

<connection>
  <from>/baseControl/odometry:o</from>
  <to>/robotGoto/odometry:i</to>
  <protocol>udp</protocol>
</connection>

</application>
