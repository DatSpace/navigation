<application>
<name>freeFloorViewer_cer02</name>

<dependencies>
</dependencies>

<module>
   <name>yarpdev</name>
   <parameters>--device transformServer --ROS "(enable_ros_publisher 1)" "(enable_ros_subscriber 1)"</parameters>
   <node>console</node>
</module>

<module>
   <name>yarprobotstatepublisher</name>
   <parameters>--model /home/elandini/.local/share/YARP/rosLaunch/cer.urdf</parameters>
   <node>console</node>
</module>

<module>
   <name>baseControl</name>
   <parameters>--context baseControl --from baseCtrl_cer.ini --skip_robot_interface_check</parameters>
   <node>console</node>
</module>

<module>
   <name>yarpdev</name>
   <parameters>--device map2DServer --mapCollectionContext mapsExample</parameters>
   <node>console</node>
</module>

<module>
   <name>yarpdev</name>
   <parameters>--device localization2DServer --subdevice odomLocalizer --context freeFloorViewer --from odomLocalizer_cer02.ini</parameters>
   <node>console</node>
</module>

<module>
   <name>yarpdev</name>
   <parameters>--device navigation2DServer --subdevice robotPathPlannerDev --context freeFloorViewer --from robotPathPlanner_cer02.ini</parameters>
   <node>console</node>
</module>

<module>
   <name>freeFloorViewer</name>
   <parameters>--context freeFloorViewer --from freeFloorViewer_cer02.ini</parameters>
   <node>console</node>
</module>

<module>
   <name>yarpview</name>
   <parameters>--name /view/floorView:i --out /view/floorClick:o</parameters>
   <node>console</node>
</module>

<connection>
  <from>/freeFloorViewer/floorEnhanced:o</from>
  <to>/view/floorView:i</to>
  <protocol>unix_stream</protocol>
</connection>

<connection>
  <from>/view/floorClick:o</from>
  <to>/freeFloorViewer/clicked_pos:i</to>
  <protocol>tcp</protocol>
</connection>

<connection>
  <from>/robotGoto/control:o</from>
  <to>/baseControl/control:i</to>
  <protocol>udp</protocol>
</connection>

<connection>
  <from>/robotPathPlanner/commands:o</from>
  <to>/robotGoto/rpc</to>
  <protocol>tcp</protocol>
</connection>

</application>
